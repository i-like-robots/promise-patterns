{
  "name": "promise-patterns",
  "version": "0.0.9",
  "description": "Utility functions to help compose promise based code.",
  "engines": {
    "node": ">=6.9.0"
  },
  "main": "dist/bundle-cjs.js",
  "module": "dist/bundle-es6.js",
  "scripts": {
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "npm run lint && npm run spec",
    "spec": "mocha 'tests/spec/*.js'",
    "lint": "standard lib/**/*.js tests/spec/**/*.js",
    "build": "rollup -c rollup.config.js"
  },
  "author": "Matt Hinchliffe",
  "repository": {
    "type": "git",
    "url": "git@github.com:i-like-robots/promise-patterns.git"
  },
  "homepage": "https://github.com/i-like-robots/promise-patterns",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "mocha": "^5.0.0",
    "rollup": "^0.55.0",
    "rollup-plugin-typescript": "^0.8.1",
    "sinon": "^4.2.1",
    "standard": "^10.0.3",
    "typescript": "^2.6.2"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "it",
      "beforeEach",
      "afterEach"
    ]
  }
}
